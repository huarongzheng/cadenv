
if ( $?MODULE_VERSION ) then
	setenv MODULE_VERSION_STACK 	"$MODULE_VERSION"
else
	setenv MODULE_VERSION 		"3.1.6"
	setenv MODULE_VERSION_STACK 	"3.1.6"
endif

set os_sys=`uname -s`
set os_rel=`uname -r | sed -e 's/^\([^.]*\.[^.]*\).*/\1/'`
set oss_osr = ${os_sys}_$os_rel
set exec_prefix='/tools/Modules/$MODULE_VERSION/$oss_osr'
unset os_sys
unset os_rel
# unset oss_osr

#
# If TCL_LIBRARY is not set
#
if ( $?TCL_LIBRARY ) then

if ( $?histchars ) then
  set histchar = `echo $histchars | cut -c1`
  set _histchars = $histchars
  alias module 'unset histchars; \\
eval `'$exec_prefix'/bin/modulecmd tcsh '$histchar'*`; \\
set histchars = $_histchars'
  unset histchar
else
  alias module 'eval `'$exec_prefix'/bin/modulecmd tcsh \!*`'
endif

else

if ( $?histchars ) then
  set histchar = `echo $histchars | cut -c1`
  set _histchars = $histchars
  alias module 'unset histchars; \\
eval `'$exec_prefix'/bin/modulecmd tcsh '$histchar'*`; \\
set histchars = $_histchars'
  unset histchar
else
  alias module 'eval `setenv TCL_LIBRARY /tools/Modules/$MODULE_VERSION/tcl8.3;'$exec_prefix'/bin/modulecmd tcsh \!*; if ( $?TCL_LIBRARY && $TCL_LIBRARY == /tools/Modules/$MODULE_VERSION/tcl8.3 ) unsetenv TCL_LIBRARY `'
endif

endif
unset exec_prefix

setenv MODULESHOME /tools/Modules/3.1.6

if (! $?MODULEPATH ) then
  setenv MODULEPATH `sed 's/#.*$//' ${MODULESHOME}/init/.modulespath | awk 'NF==1{printf("%s:",$1)}'`
endif

if (! $?LOADEDMODULES ) then
  setenv LOADEDMODULES
endif
