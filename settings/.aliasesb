#!/bin/bash
#### Aliases
alias cp='cp -r'
alias rm='\rm -rf'
alias su='su -l'
alias grep='grep -n --color=auto'
if [ `uname` = "Darwin" ]; then
  alias ls='ls -G'
else
  alias ls='ls --color=auto'
fi
alias l='ls -a'
alias ll='ls -al'
alias lld='ls -ald *'
alias vi=vim
alias rd='rdesktop -g1390x970'
alias ot='octave -qf'
alias oo='openoffice.org'
function set_desktop() {
    #cvt 1920 1200 60; #cvt "$@";
    xrandr --newmode "1920x1200_60.00"  193.25  1920 2056 2256 2592  1200 1203 1209 1245 -hsync +vsync;
    xrandr --newmode "1640x1024_60.00"  139.00  1640 1744 1912 2184  1024 1027 1037 1063 -hsync +vsync;
    xrandr --newmode "1304x768_60.00"   81.50  1304 1376 1504 1704  768 771 781 798 -hsync +vsync;
    xrandr --addmode VIRTUAL1 "1920x1200_60.00"
    xrandr --addmode VIRTUAL1 "1640x1024_60.00"
    xrandr --addmode VIRTUAL1 "1304x768_60.00"
    xrandr -s "1304x768_60.00";
}

